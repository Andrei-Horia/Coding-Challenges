#Merge The Tools
def merge_the_tools(str,num):
    vec = []

    for i in range(0,len(str), num):
        vec.append(str[i:i+num])

    for i in vec:
        d = dict()
        d.clear()
        s = ""
        
        for j in i:
            if not (j in d):
                d[j] = 1
                s += j
                
                
                
        print(s)
            


